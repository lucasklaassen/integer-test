    "aws-sdk": "^2.718.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.1",
    "middy": "^0.36.0",
    "moment": "^2.27.0",
    "npm": "^6.14.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.59",
    "@types/chai": "^4.2.11",
